	:INSERT-MODE(CTRL-T)=^t:\
	:INSERT-MODE(INS)=\E[2~:\
#       :CUT(F10)=\EOY:\
        :COPY(F11)=\EOZ:\
        :PASTE(F12)=\EO[:\
        :BACKSPACE(DEL-CHAR)=\EW:\
	:BACKSPACE(BACKSPACE)=\177:\
	:BLOCK(CTRL-V)=^v:\
	:HOME(ESC-<)=\E[1~:\
	:END(ESC->)=\E[4~:\
	:is=\E>\E[?3l\E[?4l\E[m\E[?7h\E[?8h\E(B\E)0:\
	:nd=2\E[C:\
	:do=\E[B:\
	:cl=50\E[;H\E[2J:\
	:cm=5\E[%i%d;%dH:\
	:so=2\E[7m:\
	:DELETE-COLUMN(ESC-CTRL-Z)=\E[4:\
	:DELETE-CHARACTER(DEL)=\E[3~:\
	:se=2\E[m:\
	:us=2\E[4m:\
	:ue=2\E[m:\
	:GS=^N:\
	:GE=^O:\
	:G1=k:\
	:G2=l:\
	:G3=m:\
	:G4=j:\
	:GC=n:\
	:GD=w:\
	:GH=q:\
	:GL=u:\
	:GR=t:\
	:GU=v:\
	:GV=x:\
	:HS=2\E[1m:\
	:HR=2\E[m:\
	:BB=2\E[5m:\
	:BR=2\E[m:\
	:ks=\E[?1h\E=:\
	:ke=\E[?1l\E>:\
	:cd=10\E[J:\
	:ce=10\E[K:\
	:co#110:\
	:kd=\E[B:\
	:kl=\E[D:\
	:kr=\E[C:\
	:ku=\E[A:\
	:li#40:\
	:up=\E[A:\
	:xi:\
	:cs=\E[%i%d;%dr:\
	:sr=\EM:\
	:sf=\n:\
	:GO(PF1)=\EOP:\
	:HELP(PF2)=\EOQ:\
	:ENTER-MENUBAR(PF3)=\EOR:\
	:END-ERROR(PF4)=\EOS:\
	:PAGE-UP(PAGE-UP)=\E[5~:\
	:PAGE-DOWN(PAGE-DOWN)=\E[6~:\
	:PAGE-UP(ESC-UP-ARROW)=\E\E[A:\
	:PAGE-DOWN(ESC-DOWN-ARROW)=\E\E[B:\
	:LEFT-END(ESC-LEFT-ARROW)=\E\E[D:\
	:RIGHT-END(ESC-RIGHT-ARROW)=\E\E[C:\
	:ku=\E[A:	:L_ku=<Up>:\
	:kd=\E[B:	:L_kd=<Down>:\
	:kr=\E[C:	:L_kr=<Right>:\
	:kl=\E[D:	:L_kl=<Left>:\
	:bc=\177:       :.L_bc:\
	:kh=\Eh:	:L_kh=<ESC> h:\
	:EN=\Ee:	:L_EN=<ESC> e:\
	:PU=^U: 	:L_PU=<CTRL-U>:\
	:PD=^K: 	:L_PD=<CTRL-D>:\
	:ki=\Ei:	:L_ki=<ESC> i:\
	:DL=^X: 	:L_DL=<CTRL-X>:\
	:ESC=\E\E:	:L_ESC=<ESC> <ESC>:\
	:bt=\Eb:	:L_bt=<ESC> b:\
	:fk4=\EOP:	:L_fk4=<PF1>:\
	:fk1=\EOQ:	:L_fk1=<PF2>:\
	:fk2=\EOR:	:L_fk2=<PF3>:\
	:fk3=\EOS:	:L_fk3=<PF4>:\
	:fk5=\E6:	:L_fk5=<ESC> 6:\
	:fk6=\E7:	:L_fk6=<ESC> 7:\
	:fk7=\E8:	:L_fk7=<ESC> 8:\
        :Aka=^k:      :L_Aka=Ctrl-K:\
        :Akd=^z:      :L_Akd=Ctrl-Z:\
        :Akp=^r:      :L_Akp=Ctrl-R:\
        :Aks=^l:      :L_Aks=Ctrl-L:\
        :Aku=\Em:      :L_Aku=Esc-M:\
        :Akw=^g:      :L_Akw=Ctrl-G:\
        :Aki=^e:      :L_Aki=Ctrl-E:\
	:tc=v7kf:
