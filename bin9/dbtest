#!/bin/bash
echo "*****************************************"
echo "------------Основные базы данных---------"
echo "*****************************************"
echo "------------------ Астана ---------------"
proshut /data/9/ast/bank -C list | egrep -v 'superman|inbank|wdog|biw|apw|aiw'
echo "------------------ Уральск --------------"
proshut /data/9/url/bank -C list | egrep -v 'superman|inbank|wdog|biw|apw|aiw'
